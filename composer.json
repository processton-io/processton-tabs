{
    "name": "processton/processton-tabs",
    "description": "Provide tabs schema for processton-client.",
    "version": "1.0.0",
    "keywords": [
        "processton",
        "processton-tabs"
    ],
    "homepage": "https://github.com/processton/processton-tabs",
    "license": "GPL-2.0-or-later",
    "type": "library",
    "authors": [
        {
            "name": "Ahmad Kokab",
            "email": "ahmadkokab@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "inertiajs/inertia-laravel": "^0.6.9",
        "processton-io/processton-interaction": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Processton\\ProcesstonTabs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Processton\\ProcesstonTabs\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Processton\\ProcesstonTabs\\ProcesstonTabsServiceProvider"
            ],
            "aliases": {
                "ProcesstonTabs": "Processton\\ProcesstonTabs\\ProcesstonTabsFacade"
            }
        }
    }
}
